# Unixtools setup
# Copy the specified command files is usual command directories which are included in the PATH

sudo rsync notes /usr/local/bin/notes
sudo chown root: /usr/local/bin/notes
sudo chmod 755 /usr/local/bin/notes

sudo rsync regis /usr/local/bin/regis
sudo chown root: /usr/local/bin/regis
sudo chmod 755 /usr/local/bin/regis
